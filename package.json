{
  "name": "@squirrel-forge/twighouse",
  "version": "0.9.0",
  "description": "A simple, but extendable, json+twig render tool for node",
  "main": "src/classes/TwigHouse.js",
  "scripts": {
    "setup": "node src/install.js",
    "postinstall": "npm run-script setup",
